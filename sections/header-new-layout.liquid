<div class="header-new-layout">
 <div class="header-inner page-width">
  <div class="header-new-wrap">
   {% if settings.logo != blank %}
   <a href="{{ routes.root_url }}" class="h-n-logo">
    <img src="{{ settings.logo | img_url: 'master' }}" width="{{ settings.logo_width }}" height="100%">
   </a>
   {% endif %}
   {% if section.settings.mid_text != blank %}
    <span class="header-midd-text small-hide medium-hide">{{section.settings.mid_text }}</span>
   {% endif %}
   {% if section.settings.btn_txt != blank %}
    <a href="{{ section.settings.btn_link }}" class="header_main_btn small-hide medium-hide">
      {{ section.settings.btn_txt }}
     <svg width="24px" height="24px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" transform="rotate(0)"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round" stroke="#CCCCCC" stroke-width="1.584"></g><g id="SVGRepo_iconCarrier"> <path d="M16.3153 16.6681C15.9247 17.0587 15.9247 17.6918 16.3153 18.0824C16.7058 18.4729 17.339 18.4729 17.7295 18.0824L22.3951 13.4168C23.1761 12.6357 23.1761 11.3694 22.3951 10.5883L17.7266 5.9199C17.3361 5.52938 16.703 5.52938 16.3124 5.91991C15.9219 6.31043 15.9219 6.9436 16.3124 7.33412L19.9785 11.0002L2 11.0002C1.44772 11.0002 1 11.4479 1 12.0002C1 12.5524 1.44772 13.0002 2 13.0002L19.9832 13.0002L16.3153 16.6681Z" fill="#0F0F0F"></path> </g></svg>
    </a>
   {% endif %}
   <div class="mob-burger-nav large-up-hide">
   <div id="mobile-burger">
    <span></span>
    </div>
    <div id="menu">
    <ul>
    <li>
      {%if section.settings.mid_text_mob != blank %}
      {{ section.settings.mid_text_mob }}
    {% elsif section.settings.mid_text != blank %}
      {{ section.settings.mid_text }}
    {% endif %}
    </li>
    <li>
      <a href="{{ section.settings.btn_link }}" class="header_main_btn">
       {{ section.settings.btn_txt }}
       <svg width="64px" height="64px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" transform="rotate(0)"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round" stroke="#CCCCCC" stroke-width="1.584"></g><g id="SVGRepo_iconCarrier"> <path d="M16.3153 16.6681C15.9247 17.0587 15.9247 17.6918 16.3153 18.0824C16.7058 18.4729 17.339 18.4729 17.7295 18.0824L22.3951 13.4168C23.1761 12.6357 23.1761 11.3694 22.3951 10.5883L17.7266 5.9199C17.3361 5.52938 16.703 5.52938 16.3124 5.91991C15.9219 6.31043 15.9219 6.9436 16.3124 7.33412L19.9785 11.0002L2 11.0002C1.44772 11.0002 1 11.4479 1 12.0002C1 12.5524 1.44772 13.0002 2 13.0002L19.9832 13.0002L16.3153 16.6681Z" fill="#0F0F0F"></path> </g></svg>
      </a>
    </li>
    </ul>
    </div>
    </div>
  </div>
 </div>
</div>
<script>
 var mobburger = document.getElementById('mobile-burger');
 var innercontent = document.getElementById('menu');
 mobburger.addEventListener('click',function(mobburger){
   this.classList.toggle('close');
   innercontent.classList.toggle('overlay');
});
</script>
{% schema %}
{
  "name": "Header New Layout",
  "class": "header-new-layout",
  "settings": [
    {
    "type": "text",
    "id": "mid_text",
    "label": "Middle Text Desktop"
    },
    {
      "type": "text",
      "id": "mid_text_mob",
      "label": "Middle Text Mobile"
    },
    {
     "type": "text",
     "id": "btn_txt",
     "label": "Button Text"
    },
    {
    "type": "url",
    "id": "btn_link",
    "label": "Link"
    }
  ],
  "presets": [
    {
     "name": "Header New Layout"
    }
  ]
}
{% endschema %}